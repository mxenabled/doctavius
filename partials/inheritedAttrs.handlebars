{{#if extended_from}}
<tr>
  <td>
    <strong>{{name}}</strong>
    {{#if extended_from}}
    <a href="{{crossLinkRaw extended_from}}#attribute_{{name}}">({{extended_from}})</a>
    {{/if}}
    {{#if required}}<br/>
    <span class="required"></span>
    {{/if}}
    {{#if static}}
    <span class="fa fa-bolt" title="static"></span>
    {{/if}}
    {{#if deprecated}}
    <span class="fa fa-exclamation-circle" title="deprecated"></span>
    {{/if}}
  </td>

  <td>
    {{default}}
  </td>

  <td>
    <em>{{#crossLink type}}{{/crossLink}}</em>
  </td>

  <td>
  {{{attrDescription}}}
  </td>
</tr>
{{/if}}