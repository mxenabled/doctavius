{{#if deprecated}}
<div class="deprecated">
  <h2>
    <span class="fa fa-warning"></span>Deprecated
    {{#if deprecationMessage}}
      : {{deprecationMessage}}
    {{/if}}
  </h2>
</div>
{{/if}}

<div class="pageHeader">
  <h1>
    {{name}}
  </h1>
  <div class="description">{{{classDescription}}}</div>

  <div class="meta">
    {{#if uses}}
      <div class="uses">
        Uses
        <ul class="inline commas">
          {{#uses}}
            <li><a href="{{.}}.html">{{.}}</a></li>
          {{/uses}}
        </ul>
      </div>
    {{/if}}

    <div class="extends">
      <span class="fa fa-expand"></span> {{#crossLink extends}}{{/crossLink}}
    </div>

    {{#if foundAt}}
      <div class="foundat">
        <a href="{{foundAt}}"><span class="fa fa-file"></span> `{{{file}}}:{{{line}}}`</a>
      </div>
    {{/if}}

    {{#if module}}
      <div class="module">
        {{#if submodule}}
          {{#crossLinkModule module}}<span class="fa fa-cube"></span> {{module}}{{/crossLinkModule}} <i class="fa fa-angle-right"></i> {{#crossLinkModule submodule}}{{/crossLinkModule}}
        {{else}}
          {{#crossLinkModule module}}<span class="fa fa-cube"></span> {{module}}{{/crossLinkModule}}
        {{/if}}
      </div>
    {{/if}}

    {{#if since}}
      <div class="since">
        <span class="fa fa-calendar"></span> {{since}}
      </div>
    {{/if}}
  </div>

  
</div>

<div class="boxes">
  {{#is_constructor}}
    {{#if example}}
      <div class="example col dw100">
        <div class="boxed">
          <h2>Example</h2>
          <div class="boxedContent">
            {{{example}}}
          </div>
        </div>
      </div>
    {{/if}}
  {{/is_constructor}}
  
  {{#if properties}}
    <div class="properties col dw100">
      <div class="boxed">
        <h2>Options</h2>

        <table>
          <thead>
            <th>Name</th>
            <th>Default</th>
            <th>Type</th>
            <th>Description</th>
          </thead>
          <tbody>
            {{#properties}} 
              {{>props}}
            {{/properties}}
          </tbody>
        </table>
      </div>
    </div>  
  {{#if extends}}
    <div class="properties col dw100">
      <div class="boxed">
        <h2>Inherited Options</h2>

        <table>
          <thead>
            <th>Name</th>
            <th>Default</th>
            <th>Type</th>
            <th>Description</th>
          </thead>
          <tbody>
            {{#properties}} 
              {{>inheritedProps}}
            {{/properties}}
          </tbody>
        </table>
      </div>
    </div>
  {{/if}}
  {{/if}}

  {{#if attrs}}
    <div class="attrs col dw100">
      <div class="boxed">
        <h2>Attributes</h2>

        <table>
          <thead>
            <th>Name</th>
            <th>Default</th>
            <th>Type</th>
            <th>Description</th>
          </thead>
          <tbody>
            {{#attrs}}
              {{>attrs}}
            {{/attrs}}
          </tbody>
        </table>
      </div>
    </div>
  {{#if extends}}
    <div class="attrs col dw100">
      <div class="boxed">
        <h2>Inherited Attributes</h2>

        <table>
          <thead>
            <th>Name</th>
            <th>Default</th>
            <th>Type</th>
            <th>Description</th>
          </thead>
          <tbody>
            {{#attrs}}
              {{>inheritedAttrs}}
            {{/attrs}}
          </tbody>
        </table>
      </div>
    </div>
  {{/if}}
  {{/if}}

  {{#if events}}
    <div class="events col dw100">
      <div class="boxed">
        <h2>Events</h2>

        <div class="boxedContent">
        {{#events}}
          {{>events}}
        {{/events}}
        </div>
      </div>
    </div>
  {{#if extends}}
    <div class="events col dw100">
      <div class="boxed">
        <h2>Inherited Events</h2>

        <div class="boxedContent">
        {{#events}}
          {{>inheritedEvents}}
        {{/events}}
        </div>
      </div>
    </div>
  {{/if}}
  {{/if}}

  {{#if methods}}
    <div class="methods col dw100">
      <div class="boxed">
        <h2>Methods</h2>

        <div class="boxedContent">
        {{#methods}}
          {{>method}}
        {{/methods}}
        </div>
      </div>
    </div>
  {{#if extends}}
    <div class="methods col dw100">
      <div class="boxed">
        <h2>Inherited Methods</h2>

        <div class="boxedContent">
        {{#methods}}
          {{>inheritedMethod}}
        {{/methods}}
        </div>
      </div>
    </div>
  {{/if}}
  {{/if}}
</div>
