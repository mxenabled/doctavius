{{#unless extended_from}}
<tr class="property{{#if deprecated}} deprecated{{/if}}">
  <td>
    <strong>
      {{#if overwritten_from}}
        <a href="{{crossLinkRaw overwritten_from.class}}#property_{{name}}">{{name}}</a> {{#if required}}<br/><span class="required"></span>{{/if}}
      {{else}}
        {{name}} {{#if required}}<br/><span class="required"></span>{{/if}}
      {{/if}}
    </strong>
    {{#if static}}
      <span class="fa fa-bolt" title="static"></span>
    {{/if}}
    {{#if deprecated}}
      <span class="fa fa-exclamation-circle" title="deprecated"></span>
    {{/if}}
  </td>

  <td>
    {{{default}}}
  </td>

  <td>
    <em>{{#crossLink type}}{{/crossLink}}</em>
  </td>

  <td>
    {{{description}}}
  </td>
</tr>
{{/unless}}