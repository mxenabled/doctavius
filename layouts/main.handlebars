<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>{{htmlTitle}}</title>
    
    <link rel="stylesheet" href="{{projectAssets}}/vendor/fontAwesome/css/font-awesome.css">
    <link href='//fonts.googleapis.com/css?family=Roboto:100,100italic,300,300italic,500,500italic|Inconsolata|Pacifico' rel='stylesheet' type='text/css'>
    
    <link rel="stylesheet" href="{{projectAssets}}/css/main.css">
    <link rel="stylesheet" href="{{projectAssets}}/css/prettify-custom.css">

    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    <script src="//yui.yahooapis.com/3.17.2/build/yui/yui-min.js"></script>
    <script src="{{projectAssets}}/js/main.js"></script>
    <script src="{{projectAssets}}/js/handlebarsHelpers.js"></script>
    <script src="{{projectAssets}}/js/prettify.js"></script>
    <script src="{{projectAssets}}/js/search.js"></script>
</head>
<body>
  <div id="urlRoot" data-url="{{projectRoot}}" style="display: none;"></div>
  <nav>
    <div class="logoWrapper">
      <a href="{{projectRoot}}" class="logo">

      {{#if projectThemeOptions.logo}}
        <img src="{{projectThemeOptions.logo}}" alt="{{title}}" />
      {{else}}
        {{projectName}}
      {{/if}}
      </a>

      <span class="menuToggleContainer"><span class="menuToggle open"></span></span>
    </div>
    
    {{>sidebar}}
  </nav>

    

  <div class="content row">
    <div class="topBar">
      <div class="topSearch">
        <div class="searchIcon fa fa-search"></div>
        <input class="searchInput" value="" placeholder="Search the docs..."/>
        <ul class="autoSuggest"></ul>
      </div>

      <div class="topNav">
        {{#if projectRepo}}
        <a href="{{projectRepo}}" class="fa fa-code-fork"></a>
        {{/if}}

        {{#each projectNav}}
        <a href="{{link}}" class="fa {{icon}}"></a>
        {{/each}}
      </div>
    </div>
    {{>layout_content}}
  </div>


</body>
</html>
